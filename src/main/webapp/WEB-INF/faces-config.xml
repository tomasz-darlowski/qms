<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <lifecycle>
        <phase-listener id="nocache">qmsjee.listeners.CacheControlPhaseListener</phase-listener>
    </lifecycle>

    <application>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>pl</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>bundle.messages</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>




    <!--***********************NAVIGATION**************************-->

    <!--//LOGIN//-->
    <navigation-rule>
        <from-view-id>/Login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>signup</from-outcome>
            <to-view-id>/SignUp.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>remind</from-outcome>
            <to-view-id>/Remind.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <!--//MAIN//-->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>userAdmin</from-outcome>
            <to-view-id>/admin/ManageUser.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>customer</from-outcome>
            <to-view-id>/app/Collections/Customer.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>issues</from-outcome>
            <to-view-id>/app/Issues/Issues.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>documents</from-outcome>
            <to-view-id>/app/Documents/Documents.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>supplier</from-outcome>
            <to-view-id>/app/Collections/Supplier.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>scheduler</from-outcome>
            <to-view-id>/app/Scheduler/Scheduler.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/Login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>components</from-outcome>
            <to-view-id>/app/Components/Components.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>gauges</from-outcome>
            <to-view-id>/app/Gauges/Gauges.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/app/Dashboard/Dashboard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <!--//SCHEDULLER//-->
    <navigation-rule>
        <from-view-id>/app/Scheduler/Scheduler.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/app/Scheduler/Create.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/app/Scheduler/Edit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>view</from-outcome>
            <to-view-id>/app/Scheduler/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/app/Scheduler/Invitation.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>view</from-outcome>
            <to-view-id>/app/Scheduler/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/app/Scheduler/View.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/app/Scheduler/Edit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/app/Scheduler/Edit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>view</from-outcome>
            <to-view-id>/app/Scheduler/View.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <!--//COMPONENTS//-->
    <navigation-rule>
        <from-view-id>/app/Components/Components.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/app/Components/Create.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/app/Components/Edit.xhtml</to-view-id>
            <redirect>
                <view-param>
                    <name>item</name>
                    <value>#{componentMainMBean.selectedComponentReference}</value>
                </view-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>view</from-outcome>
            <to-view-id>/app/Components/View.xhtml</to-view-id>
            <redirect>
                <view-param>
                    <name>item</name>
                    <value>#{componentMainMBean.selectedComponentReference}</value>
                </view-param>
